{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "website",
        "image": "first-container-image",
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 8080,
            "containerPort": 8080
          }
        ]
      },
      {
        "name": "ui",
        "image": "second-container-image",
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ]
      }
    ]
  }
  